@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <table style="border-collapse: collapse;" border="1" cellpadding="5" cellspacing="0">
        @for (int i = 1; i <= 12; i++)
        {
            <tr>
                @for (int j = 1; j <= 10; j++)
                {
                    <td style="border: 1px solid black;">@i * @j = @(i* j)</td>
                }
            </tr>
        }
    </table>
</div>
@{
}

@model List<_01_ViewBag.Models.Entities.clsDepartamento>


<table border="1" class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var departamento in Model)
        {
            <tr>
                <td>@departamento.idDepartamento</td>
                <td>@departamento.nombreDepartamento</td>
            </tr>
        }
    </tbody>
</table>
@Html.ActionLink("Link Index", "Index", "Home")